<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane alignment="center" hgap="10" vgap="10">
        <padding><Insets top="25" right="25" bottom="10" left="25"/></padding>

        <Label text="Team Name" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <TextField fx:id="teamNameBox" GridPane.columnIndex="1" GridPane.rowIndex="0" />

        <Button fx:id="enterButton" text="Enter" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#enterButtonAction" />

        <ListView fx:id="teamListView" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    </GridPane>
</VBox>
