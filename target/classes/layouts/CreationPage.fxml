<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:controller="CreationPageController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:id="borderPane">
    <top>
        <HBox spacing="30">
            <Button fx:id="createRegattaButton" text="Create Regatta" onAction="#createRegattaButtonAction" />
            <Button fx:id="createTeamButton" text="Create Team" onAction="#createTeamButtonAction" />
            <Button fx:id="createPersonButton" text="Create Person" onAction="#createPersonButtonAction" />
            <Button fx:id="createCrewButton" text="Create Crew" onAction="#createCrewButtonAction"/>
            <Button fx:id="createRaceButton" text="Create Race" onAction="#createRaceButtonAction"/>
            <Button fx:id="enterRaceTimeButton" text="Enter race times" onAction="#enterRaceTimeButtonAction"/>
            <Button fx:id="finishRegattaButton" text="Save regatta" onAction="#finishRegattaButtonAction"/>
            <Button fx:id="outputRegattaButton" text="Output regatta to PDF" onAction="#outputRegattaButtonAction"/>
        </HBox>
    </top>
    <center>
        <fx:include source="CreationPageStart.fxml"/>
    </center>
</BorderPane>
